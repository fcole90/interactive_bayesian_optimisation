{
  "compilerOptions": {
    "outDir": "../interactive_bayesian_optimisation/static/js/dist",
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
  },
  "baseUrl": ".",
  "paths": {
    "@jquery": [
      "node_modules/jquery/src/jquery"
    ],
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    ".eslintrc.cjs",
    "src/legacy/libs/std.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}